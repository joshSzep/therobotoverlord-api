# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/therobotoverlord
DATABASE_MIN_POOL_SIZE=10
DATABASE_MAX_POOL_SIZE=20
DATABASE_POOL_TIMEOUT=30
DATABASE_COMMAND_TIMEOUT=60

# Authentication Configuration
AUTH_GOOGLE_CLIENT_ID=your_google_client_id
AUTH_GOOGLE_CLIENT_SECRET=your_google_client_secret
AUTH_GOOGLE_REDIRECT_URI=http://localhost:8000/api/v1/auth/callback
AUTH_JWT_SECRET_KEY=your_jwt_secret_key_here
AUTH_JWT_ALGORITHM=HS256
AUTH_JWT_ACCESS_TOKEN_EXPIRE_MINUTES=30

# Redis Configuration
REDIS_URL=redis://localhost:6379/0
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=
REDIS_SSL=false
REDIS_SSL_CERT_REQS=none
REDIS_SSL_CA_CERTS=
REDIS_SSL_CERTFILE=
REDIS_SSL_KEYFILE=
REDIS_SOCKET_CONNECT_TIMEOUT=5
REDIS_SOCKET_KEEPALIVE=true
REDIS_SOCKET_KEEPALIVE_OPTIONS=
REDIS_CONNECTION_POOL_MAX_CONNECTIONS=50
AUTH_REFRESH_TOKEN_LIFETIME=1209600
AUTH_COOKIE_DOMAIN=.therobotoverlord.com
AUTH_COOKIE_SECURE=true
AUTH_COOKIE_SAMESITE=lax
AUTH_SESSION_CLEANUP_INTERVAL=3600
